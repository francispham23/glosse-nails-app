{
	"expo": {
		"name": "glosse-nail-app",
		"slug": "glosse-nail-app",
		"version": "0.0.3",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"scheme": "glossenailapp",
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-navigation-bar",
				{
					"backgroundColor": "#0f172a",
					"barStyle": "light",
					"borderColor": "#1f2937",
					"visibility": "visible",
					"behavior": "inset-swipe",
					"position": "relative"
				}
			]
		],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		},
		"userInterfaceStyle": "automatic",
		"extra": {
			"eas": {
				"projectId": "92d651b7-4768-4989-929a-6c8eed497950"
			}
		},
		"ios": {
			"bundleIdentifier": "com.francispham.glossenailapp",
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false
			}
		}
	}
}
