{
	"expo": {
		"name": "glosse-nail-app",
		"slug": "glosse-nail-app",
		"version": "0.0.1",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"scheme": "glossenailapp",
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"backgroundColor": "#ffffff",
					"image": "./assets/logo-b.png",
					"dark": {
						"image": "./assets/logo-w.png",
						"backgroundColor": "#000000"
					}
				}
			],
			"expo-secure-store",
			"expo-web-browser"
		],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true,
			"tsconfigPaths": true
		},
		"newArchEnabled": true,
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"]
	}
}
